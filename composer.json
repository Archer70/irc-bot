{
    "name": "wildphp/wild-irc-bot",
    "description": "A modular IRC bot written in PHP",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "nette/neon": "^2",
        "react/event-loop": "^0.4",
        "react/socket": "^0.8",
        "fire015/flintstone": "^2.1",
        "evenement/evenement": "^2.0",
        "yoshi2889/collections": "^0.1",
        "yoshi2889/container": "^0.1",
        "yoshi2889/validation-closures": "^0.1",
