      "wildphp/module-autorejoin": "dev-master",
      "wildphp/module-factoids": "dev-master",
      "wildphp/module-linksniffer": "dev-master",
      "wildphp/module-russianroulette": "dev-master",
      "wildphp/module-tgrelay": "dev-master",
      "wildphp/module-uno": "dev-master",
